<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Carina Demo - Web Tests" verbose="1">
    <parameter name="suiteOwner" value="Tester"/>
    <parameter name="jenkinsJobName" value="Web-Demo-Test"/>
    <parameter name="jenkinsJobType" value="web"/>
    <parameter name="jenkinsEnvironments" value="DEMO"/>
    <parameter name="jenkinsRegressionPipeline" value="web_regression"/>
    <parameter name="jenkinsAutoScreenshot" value="true"/>

    <!-- Specify custom properties files -->
    <parameter name="configFile" value="src/main/resources/_config.properties"/>
    <parameter name="env" value="DEMO"/>

    <test name="Automation Exercise Web Tests">
        <parameter name="test_platform" value="web"/>
        <classes>
            <class name="com.zebrunner.carina.automationexercise.web.automationexercise.WebProductTest"/>
            <class name="com.zebrunner.carina.automationexercise.web.automationexercise.WebCartTest"/>
            <class name="com.zebrunner.carina.automationexercise.web.automationexercise.WebContactTest"/>
            <class name="com.zebrunner.carina.automationexercise.web.automationexercise.WebRecommendedItemsTest"/>
            <class name="com.zebrunner.carina.automationexercise.web.automationexercise.WebSubscriptionTest"/>
            <class name="com.zebrunner.carina.automationexercise.web.WebCrossBrowserTest"/>
        </classes>
    </test>
</suite>