<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Carina Demo - iOS Tests" verbose="1">
    <parameter name="suiteOwner" value="Tester"/>
    <parameter name="jenkinsJobName" value="iOS-Demo-Test"/>
    <parameter name="jenkinsJobType" value="ios"/>
    <parameter name="jenkinsEnvironments" value="DEMO"/>
    <parameter name="jenkinsRegressionPipeline" value="mobile_regression"/>
    <parameter name="jenkinsAutoScreenshot" value="true"/>
    <parameter name="jenkinsMobileDefaultPool" value="ANY"/>

    <parameter name="jenkinsMobileLifecycle" value="false"/>
    <parameter name="jenkinsEmail" value="demo@gmail.com"/>
    <parameter name="jenkinsJobExecutionMode" value="continue"/>

    <!-- Specify custom properties files -->
    <parameter name="configFile" value="src/main/resources/_config.properties,src/main/resources/ios.properties"/>
    <parameter name="env" value="DEMO"/>

    <test name="iOS Mobile Tests">
        <parameter name="test_platform" value="ios"/>
        <classes>
            <!-- Добавьте здесь iOS тесты, когда они будут созданы -->
            <!-- <class name="com.zebrunner.carina.automationexercise.mobile.IOSSampleTest"/> -->
        </classes>
    </test>
</suite>